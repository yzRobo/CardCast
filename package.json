{
  "name": "cardcast",
  "version": "1.0.0",
  "description": "Streaming overlay tool for TCG content creators",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "node scripts/build-exe.js",
    "test": "node scripts/test-setup.js",
    "setup": "npm install && node scripts/test-setup.js",
    "clean": "rimraf node_modules dist data/*.db cache/*",
    "rebuild": "npm run clean && npm install && npm run test"
  },
  "keywords": [
    "tcg",
    "streaming",
    "overlay",
    "obs",
    "pokemon",
    "magic",
    "yugioh",
    "lorcana"
  ],
  "author": "",
  "license": "GPL-3.0",
  "dependencies": {
    "axios": "^1.6.0",
    "better-sqlite3": "^9.2.2",
    "cheerio": "^1.0.0-rc.12",
    "express": "^4.18.2",
    "socket.io": "^4.6.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "pkg": "^5.8.1",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "pkg": {
    "assets": [
      "public/**/*",
      "src/**/*",
      "overlays/**/*",
      "index.html",
      "config.json"
    ],
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "dist",
    "compress": "GZip"
  }
}